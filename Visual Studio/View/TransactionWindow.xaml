<Window x:Class="View.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="Transaction" Height="250" Width="430">
    <Grid>
        <Button x:Name="deposit" Content="Deposit" HorizontalAlignment="Left" Margin="19,164,0,0" VerticalAlignment="Top" Width="115" Height="37" FontSize="18"/>
        <Button x:Name="withdraw" Content="Withdraw" HorizontalAlignment="Left" Margin="162,164,0,0" VerticalAlignment="Top" Width="111" Height="37" FontSize="18"/>
        <Button x:Name="logout" Content="Logout" HorizontalAlignment="Left" Margin="301,164,0,0" VerticalAlignment="Top" Width="103" Height="37" FontSize="18" Click="logout_Click"/>
        <TextBox HorizontalAlignment="Center" Height="79" Margin="20,68,19,73" TextWrapping="Wrap" Text="12000" VerticalAlignment="Center" Width="385" FontSize="48"/>
        <Label x:Name="sAccountNumber" Content="#123456" HorizontalAlignment="Left" Margin="28,18,0,0" VerticalAlignment="Top" Width="127" FontSize="18"/>
        <Label x:Name="Sbalance" Content="2000" HorizontalAlignment="Right" Margin="0,18,20,0" VerticalAlignment="Top" Width="135" FontSize="18"/>

    </Grid>
</Window>
